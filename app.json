{
  "expo": {
    "name": "CyClick",
    "slug": "CyClick",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/cyclick-logo.jpg",
    "splash": {
      "image": "./assets/cyclick-logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jeromejerome.CyClick"
    },
    "android": {
      "package": "com.jeromejerome.CyClick",
      "permissions": ["ACCESS_BACKGROUND_LOCATION", "ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["@react-native-mapbox-gl/maps"],
    "facebookScheme": "fb123456",
    "facebookAppId": "5999614916775320",
    "facebookDisplayName": "CyClick"
  }
}
